    <dc:title>Mercan Adası</dc:title>
    <dc:creator xmlns:ns0="http://www.idpf.org/2007/opf" ns0:role="aut" ns0:file-as="Ballantyne, Robert Michael">Robert Michael Ballantyne</dc:creator>
